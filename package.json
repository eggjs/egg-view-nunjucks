{
  "name": "egg-view-nunjucks",
  "version": "0.5.0",
  "description": "view plugin for egg",
  "eggPlugin": {
    "name": "view",
    "dep": [
      "security"
    ]
  },
  "keywords": [
    "egg",
    "plugin",
    "view",
    "template",
    "nunjucks"
  ],
  "dependencies": {
    "nunjucks": "^2.5.2"
  },
  "devDependencies": {
    "autod": "^2.7.1",
    "cheerio": "^0.22.0",
    "common-tags": "^1.3.1",
    "egg": "*",
    "egg-bin": "^1.7.0",
    "egg-ci": "^1.0.3",
    "egg-mock": "^1.1.0",
    "eslint": "^3.9.1",
    "eslint-config-egg": "^3.1.0",
    "intelli-espower-loader": "^1.0.1",
    "power-assert": "^1.4.1",
    "supertest": "^2.0.1"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test -r intelli-espower-loader",
